{
	"schemaVersion": 1,
	"id": "bnb",
	"version": "${version}",

	"name": "Better Nether Beta",
	"description": "Enhancement for the Nether dimension",
	"authors": [
		"paulevs"
	],
	"contact": {
		"homepage": "https://github.com/paulevsGitch/BetterNetherBeta/releases",
		"sources": "https://github.com/paulevsGitch/BetterNetherBeta",
		"issues": "https://github.com/paulevsGitch/BetterNetherBeta/issues"
	},

	"license": "CC0-1.0",
	"icon": "assets/bnb/icon.png",

	"environment": "*",
	"entrypoints": {
		"stationapi:event_bus": [
			"paulevs.bnb.listeners.CommonListener"
		],
		"stationapi:event_bus_client": [
			"paulevs.bnb.listeners.TextureListener",
			"paulevs.bnb.listeners.ModelListener"
		],
		"bhcreative:event_bus": [
			"paulevs.bnb.listeners.CreativeTabListener"
		]
	},

	"mixins": [
		"bnb.mixins.common.json",
		"bnb.mixins.client.json",
		"bnb.mixins.server.json"
	],
	
	"suggests": {
		"bhcreative": ">=0.2.3"
	},
	"depends": {
		"minecraft": "1.0.0-beta.7.3"
	}
}
